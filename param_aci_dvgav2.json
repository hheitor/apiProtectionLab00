{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "containerName": {
            "value": "dvgalab"
        },
        "location": {
            "value": "centralus"
        },
        "command": {
            "value": "docker run -t -p 5000:5000 -e WEB_HOST=0.0.0.0 dolevf/dvga"
        },
        "imageType": {
            "value": "Public"
        },
        "imageName": {
            "value": "dolevf/dvga"
        },
        "osType": {
            "value": "Linux"
        },
        "numberCpuCores": {
            "value": "1"
        },
        "memory": {
            "value": "1.5"
        },
        "restartPolicy": {
            "value": "OnFailure"
        },
        "ipAddressType": {
            "value": "Public"
        },
        "ports": {
            "value": [
                {
                    "port": "5000",
                    "protocol": "TCP"
                }
            ]
        },
        "dnsNameLabel": {
            "value": "hhapiprotlab"
        }
    }
}